{
  "name": "redux-devtools-cli",
  "version": "1.0.0-1",
  "description": "CLI for remote debugging with Redux DevTools.",
  "main": "index.js",
  "bin": {
    "redux-devtools": "bin/redux-devtools.js"
  },
  "files": [
    "bin",
    "src",
    "app",
    "index.js",
    "defaultDbOptions.json"
  ],
  "scripts": {
    "dev": "nodemon ./bin/redux-devtools.js",
    "start": "node ./bin/redux-devtools.js",
    "start:electron": "node ./bin/redux-devtools.js --open",
    "test": "jest",
    "prepublishOnly": "npm run test",
    "lint": "esw src bin --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch --clear"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reduxjs/redux-devtools.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reduxjs/redux-devtools/issues"
  },
  "homepage": "https://github.com/reduxjs/redux-devtools",
  "dependencies": {
    "body-parser": "^1.15.0",
    "chalk": "^1.1.3",
    "cors": "^2.7.1",
    "cross-spawn": "^6.0.5",
    "diff": "^4.0.2",
    "electron": "^4.0.1",
    "express": "^4.13.3",
    "getport": "^0.1.0",
    "graphql": "^0.13.0",
    "graphql-server-express": "^1.4.0",
    "graphql-tools": "^4.0.3",
    "knex": "^0.15.2",
    "lodash": "^4.15.0",
    "loglevel": "^1.6.8",
    "loglevel-plugin-prefix": "^0.8.4",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "opn": "^5.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "redux-devtools-core": "file:./redux-devtools-core",
    "semver": "^5.3.0",
    "socketcluster": "^14.3.3",
    "sqlite3": "^4.0.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-watch": "^6.0.1",
    "jest": "^24.1.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "socketcluster-client": "^14.0.0",
    "supertest": "^3.0.0"
  }
}
